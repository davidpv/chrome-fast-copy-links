{
  "manifest_version": 2,
  "name": "Fast Copy links",
  "version": "1",
  "description": "Fast Copy all selected links from right menu",
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  }
}